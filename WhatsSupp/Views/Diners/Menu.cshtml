@model WhatsSupp.ViewModels.FullMenuVM

@{
    ViewData["Title"] = "Menu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Menu</h1>
<hr />

@for (int i = 0; i < Model.AllMenuPages.Count; i++)
{
    @for (int j = 0; j < Model.AllMenuPages[i].result.numResults; j++)
    {
        <h4>@Model.AllMenuPages[i].result.data[j].menu_item_name</h4>
        <p style="padding-left: 10px">@Model.AllMenuPages[i].result.data[j].menu_item_description</p>
    }
       
}
        
